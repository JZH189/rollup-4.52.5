{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Debug Rollup Example",
			"program": "${workspaceFolder}/example/rollup.js",
			"args": [],
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "node",
			"runtimeArgs": ["--inspect-brk"],
			"env": {},
			"console": "integratedTerminal",
			"sourceMaps": true,
			"outFiles": ["${workspaceFolder}/dist/**/*.js"]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Debug Rollup Build",
			"program": "${workspaceFolder}/cli/cli.ts",
			"args": ["--config"],
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "node",
			"runtimeArgs": ["--inspect-brk", "-r", "ts-node/register"],
			"env": {},
			"console": "integratedTerminal",
			"sourceMaps": true
		}
	]
}
